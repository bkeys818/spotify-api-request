{
    "json.schemas": [
        {
            "url": "https://json.schemastore.org/tsconfig.json",
            "fileMatch": ["*.tsconfig.json"]
        },
        {
            "url": ".vscode/template.schema.json",
            "fileMatch": ["/schemas/*.json"]
        }
    ],
    "files.associations": {
        "*.tsconfig.json": "jsonc"
    },
    "liveServer.settings.root": "docs/",
    "jest.jestCommandLine": "node_modules/.bin/jest"
}
