    "postdocs": "open http://localhost:5050 && http-server -a -s localhost -p 5050 docs",
    "prebuild": "rimraf lib & rimraf types",
    "build": "tsc --build tsconfig.src.json",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
