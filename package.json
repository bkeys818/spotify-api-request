  "main": "lib/index.js",
  "files": [
    "lib",
    "types",
    "tsconfig.json",
    "tsconfig.src.json"
  ],
  "types": "types",
  "scripts": {
    "pretest": "rimraf coverage",
    "test": "jest --coverage",
    "precoverage": "open http://localhost:8080",
    "coverage": "http-server -s -a localhost ./coverage/lcov-report",
    "predocs": "rimraf docs && mkdir docs",
