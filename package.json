    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.1",
    "@types/node-fetch": "^2.5.10",
    "babel-jest": "^27.0.2",
    "dotenv": "^10.0.0",
    "http-server": "^0.12.3",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "typedoc": "^0.21.0",
    "typescript": "^4.3.2"
  }
